import java.util.*;
public class Q2
{
	public static void main(String[] args) {
		System.out.println(reverse("dataStructure"));
	}
	public static String reverse(String s){
	    Stack<Character> st = new Stack<>();
	    int n = s.length();
	    for(int i=0;i<n;i++){
	        st.push(s.charAt(i));
	    }
	    String ans="";
	    for(int j=st.size()-1;j>=0;j--){
	        ans+=st.pop();
	    }
	    return ans;
	}
}